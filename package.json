    "meow": "^3.4.2",
    "parse": "^1.6.7",
    "request": "^2.65.0",
    "speed-test": "^1.5.0"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "fs-extended": "^0.2.1"
  }
