{
  "name": "net-test",
  "version": "0.1.1",
  "description": "CLI utility to test WiFi quality and save results for future reference.",
  "preferGlobal": true,
  "main": "cli.js",
  "bin": "./cli.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:meeDamian/net-test.git"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
